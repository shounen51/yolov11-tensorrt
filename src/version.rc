#include <windows.h>

// 版本號更新規則：
// MAJOR: 重大架構變更、API不相容變更
// MINOR: 新功能增加、API向下相容的變更
// PATCH: Bug修復、小幅改進、效能優化
// BUILD: 自動構建編號 (可選)
#define VER_MAJOR 1     // 重大架構變更、API破壞性變更
#define VER_MINOR 1     // 新功能、向下相容的API變更、數據不相容的更新
#define VER_PATCH 0     // Bug修復、小幅改進
#define VER_BUILD 0     // 構建編號（可選）

// 自動生成版本格式 - 不需要修改以下內容
#define VER_FILEVERSION VER_MAJOR,VER_MINOR,VER_PATCH,VER_BUILD
#define VER_PRODUCTVERSION VER_MAJOR,VER_MINOR,VER_PATCH,VER_BUILD

#define STRINGIFY(x) #x
#define TOSTRING(x) STRINGIFY(x)
#define VER_FILEVERSION_STR TOSTRING(VER_MAJOR) "." TOSTRING(VER_MINOR) "." TOSTRING(VER_PATCH) "." TOSTRING(VER_BUILD)
#define VER_PRODUCTVERSION_STR TOSTRING(VER_MAJOR) "." TOSTRING(VER_MINOR) "." TOSTRING(VER_PATCH)

VS_VERSION_INFO VERSIONINFO
FILEVERSION     VER_FILEVERSION
PRODUCTVERSION  VER_PRODUCTVERSION
FILEFLAGSMASK   VS_FFI_FILEFLAGSMASK
FILEFLAGS       0
FILEOS          VOS__WINDOWS32
FILETYPE        VFT_DLL
FILESUBTYPE     VFT2_UNKNOWN
BEGIN
    BLOCK "StringFileInfo"
    BEGIN
        BLOCK "040904E4"
        BEGIN
            VALUE "FileDescription",  "YOLOv11 TensorRT Detection Library"
            VALUE "FileVersion",      VER_FILEVERSION_STR
            VALUE "ProductVersion",   VER_PRODUCTVERSION_STR
        END
    END

    BLOCK "VarFileInfo"
    BEGIN
        VALUE "Translation", 0x409, 1252
    END
END
